第一个想法：通过map和stack来解决
class Solution:
    def isValid(self, s: str) -> bool:
        match = {'}': '{', ']': '[', ')': '('}
        l = [None]
        for i in s:
            if i in match and match[i] == l[-1]:
                l.pop()
            else:
                l.append(i)
        return len(l)==1
        
